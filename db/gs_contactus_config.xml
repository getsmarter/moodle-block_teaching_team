        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="fromroleid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
